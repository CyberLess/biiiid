extends ../layouts/base

append vars
	- title = 'Профиль(не нажал  стать исполнителем, но уже заказывал как заказчик)';
	- body.classList = "is-have-mobile-nav";

block header
	+header(true)

block content

	include ../layouts/mobile-filters

	+b.NAV.fluid._mobile
		+e.wrap.wrap
			+e.row
				+e.cell._fluid
					+e.box-button.btn__._red._big.p.A(href='#')
						+e.text Подробнее
				+e.cell._static
